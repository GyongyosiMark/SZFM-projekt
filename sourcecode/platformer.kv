<PlatformerGame>:
    canvas:
        Rectangle:
            id: player
            pos: self.pos
            size: self.pos
            source: "player.png"

        Rectangle:
            id: enemy
            pos: self.pos
            size: self.pos
            source: "enemy.png"

    Label:
        font_size: 50
        center_x: root.width/2
        top: root.top
        text: "Game"